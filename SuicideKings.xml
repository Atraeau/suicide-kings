              <Button name="SuicideKingsPlayerListButton5" inherits="SuicideKingsPlayerListButtonTemplate" id="5">
                <Anchors>
                  <Anchor point="TOPLEFT" relativeTo="SuicideKingsPlayerListButton4" relativePoint="BOTTOMLEFT">
                    <Offset>
