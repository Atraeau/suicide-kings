              <Button name="SuicideKingsPlayerListButton6" inherits="SuicideKingsPlayerListButtonTemplate" id="6">
                <Anchors>
                  <Anchor point="TOPLEFT" relativeTo="SuicideKingsPlayerListButton5" relativePoint="BOTTOMLEFT">
                    <Offset>
                      <AbsDimension x="0" y="0" />
                    </Offset>
                  </Anchor>
                </Anchors>
              </Button>
              <Button name="SuicideKingsPlayerListButton7" inherits="SuicideKingsPlayerListButtonTemplate" id="7">
                <Anchors>
                  <Anchor point="TOPLEFT" relativeTo="SuicideKingsPlayerListButton6" relativePoint="BOTTOMLEFT">
                    <Offset>
                      <AbsDimension x="0" y="0" />
                    </Offset>
                  </Anchor>
                </Anchors>
              </Button>
              <Button name="SuicideKingsPlayerListButton8" inherits="SuicideKingsPlayerListButtonTemplate" id="8">
                <Anchors>
                  <Anchor point="TOPLEFT" relativeTo="SuicideKingsPlayerListButton7" relativePoint="BOTTOMLEFT">
                    <Offset>
                      <AbsDimension x="0" y="0" />
                    </Offset>
                  </Anchor>
                </Anchors>
              </Button>
              <Button name="SuicideKingsPlayerListButton9" inherits="SuicideKingsPlayerListButtonTemplate" id="9">
                <Anchors>
                  <Anchor point="TOPLEFT" relativeTo="SuicideKingsPlayerListButton8" relativePoint="BOTTOMLEFT">
                    <Offset>
                      <AbsDimension x="0" y="0" />
                    </Offset>
                  </Anchor>
                </Anchors>
              </Button>
              <Button name="SuicideKingsPlayerListButton10" inherits="SuicideKingsPlayerListButtonTemplate" id="10">
                <Anchors>
                  <Anchor point="TOPLEFT" relativeTo="SuicideKingsPlayerListButton9" relativePoint="BOTTOMLEFT">
                    <Offset>
                      <AbsDimension x="0" y="0" />
                    </Offset>
                  </Anchor>
                </Anchors>
              </Button>
              <Button name="SuicideKingsPlayerListButton11" inherits="SuicideKingsPlayerListButtonTemplate" id="11">
                <Anchors>
                  <Anchor point="TOPLEFT" relativeTo="SuicideKingsPlayerListButton10" relativePoint="BOTTOMLEFT">
                    <Offset>
                      <AbsDimension x="0" y="0" />
                    </Offset>
                  </Anchor>
                </Anchors>
              </Button>
              <Button name="SuicideKingsPlayerListButton12" inherits="SuicideKingsPlayerListButtonTemplate" id="12">
                <Anchors>
                  <Anchor point="TOPLEFT" relativeTo="SuicideKingsPlayerListButton11" relativePoint="BOTTOMLEFT">
                    <Offset>
                      <AbsDimension x="0" y="0" />
                    </Offset>
                  </Anchor>
                </Anchors>
              </Button>
              <Button name="SuicideKingsPlayerListButton13" inherits="SuicideKingsPlayerListButtonTemplate" id="13">
                <Anchors>
                  <Anchor point="TOPLEFT" relativeTo="SuicideKingsPlayerListButton12" relativePoint="BOTTOMLEFT">
                    <Offset>
                      <AbsDimension x="0" y="0" />
                    </Offset>
                  </Anchor>
                </Anchors>
