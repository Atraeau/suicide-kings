            <Anchors>
              <Anchor point="TOPRIGHT" relativeTo="GuildListFrame" relativePoint="TOPRIGHT">
                <Offset>
                  <AbsDimension x="-32" y="-87" />
                </Offset>
              </Anchor>
            </Anchors>
            <Scripts>
              <OnVerticalScroll>
                FauxScrollFrame_OnVerticalScroll(self, offset, SK_GUILDMEMBERS_TO_HEIGHT, SuicideKings_Update);
              </OnVerticalScroll>
            </Scripts>
            <Layers>
              <Layer level="BACKGROUND">
                <Texture file="Interface\PaperDollInfoFrame\UI-Character-ScrollBar">
                  <Size>
                    <AbsDimension x="31" y="226" />
